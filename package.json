{
  "name": "@italodeandra/ui",
  "version": "0.4.1",
  "description": "Next.js UI library",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "yalc:update": "yalc update",
    "demo:dev": "next -p 3002",
    "lint": "next lint",
    "build:lib": "shx rm -rf dist && tsc --build lib/tsconfig.json && shx cp package.json dist",
    "pre-publish": "npm run build:lib && cd dist && json -I -f package.json -e \"this.devDependencies=undefined; this.optionalDependencies=undefined; this._optionalDependencies=undefined; this.scripts=undefined;\"",
    "publish:github": "npm run pre-publish && cd dist && npm publish",
    "publish:yalc": "npm run pre-publish && cd dist && yalc publish"
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.6.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/inter": "^4.5.1",
    "@iconify/icons-heroicons-outline": "^1.1.3",
    "@iconify/icons-heroicons-solid": "^1.1.3",
    "@iconify/react": "^3.0.1",
    "@italodeandra/next": "^0.5.3",
    "@mui/lab": "^5.0.0-alpha.57",
    "@mui/material": "^5.2.2",
    "lodash": "^4.17.21",
    "material-ui-popup-state": "^2.0.0",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dnd-touch-backend": "^14.1.1",
    "react-hook-form": "^7.20.4",
    "react-query": "^3.33.4",
    "react-use": "^17.3.1",
    "valtio": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@emotion/cache": ">=11.6.0",
    "@emotion/react": ">=11.6.0",
    "@emotion/server": ">=11.4.0",
    "@emotion/styled": ">=11.6.0",
    "@fontsource/inter": ">=4.5.1",
    "@iconify/icons-heroicons-outline": ">=1.1.3",
    "@iconify/icons-heroicons-solid": ">=1.1.3",
    "@iconify/react": ">=3.0.1",
    "@italodeandra/next": ">=0.5.3",
    "@mui/lab": ">=5.0.0-alpha.57",
    "@mui/material": ">=0.5.2-canary.0",
    "lodash": ">=4.17.21",
    "material-ui-popup-state": ">=2.0.0",
    "react-dnd": ">=14.0.4",
    "react-dnd-html5-backend": ">=14.0.2",
    "react-dnd-touch-backend": ">=14.1.1",
    "react-hook-form": ">=7.20.4",
    "react-query": ">=3.33.4",
    "react-use": ">=17.3.1",
    "valtio": ">=1.2.5"
  },
  "devDependencies": {
    "@italodeandra/dev": "^0.2.3-canary.0"
  },
  "_optionalDependencies": {
    "bson": "npm:bson",
    "prettier": "npm:prettier",
    "react": "npm:react"
  }
}
